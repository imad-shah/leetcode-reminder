{
    "manifest_version": 3,
    "name": "Neetcode Info Scraper",
    "version": "1.0",
    "description": "Gathers all information around current Neetcode Problem",
    "content_scripts": [
        {
        "matches": ["https://neetcode.io/*"],
        "js": ["content.js"]
        }
    ],
    "host_permissions": ["http://127.0.0.1:8000/*"]
}